{
  "name": "realtime-xat",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "build:dev": "nx build --configuration development",
    "build:api": "nx build xat-be --configuration=production",
    "build:api:dev": "nx build xat-be --configuration=development",
    "test": "nx test",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:fe": "nx serve xat-fe --configuration development",
    "start:be": "nx serve xat-be --build-target=xat-be:build:development",
    "build:all": "nx build xat-be && nx build xat-fe",
    "build:all:dev": "nx build xat-be --configuration=development && nx build xat-fe --configuration development",
    "dev": "concurrently -p=\"{name}\" -n=\"Angular,NestJS\" -c=\"green,blue\" \"npm run start:fe\" \"npm run start:be\""
  },
  "private": true,
